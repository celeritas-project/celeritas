! ORANGE testem3 input definition using universes. This geometry supports 
! simple safety
[GEOMETRY]
global "outer"

comp         : matid
    "Pb"  0
    "lAr"  1

!#### LEVEL 1 UNIVERSE ####!

[UNIVERSE=general inner]
interior "pair"

[UNIVERSE][SHAPE=cuboid pair]
faces 0 0.8 -20 20 -20 20

[UNIVERSE][SHAPE=cuboid gap_shape]
faces 0 0.23 -20 20 -20 20

[UNIVERSE][SHAPE=cuboid abs_shape]
faces 0.23 0.8 -20 20 -20 20

[UNIVERSE][CELL gap]
comp "lAr"
shapes -gap_shape

[UNIVERSE][CELL absorber]
comp "Pb"
shapes -abs_shape

[UNIVERSE=general outer]
interior "full"

!#### LEVEL 0 UNIVERSE ####!

