! Diagram (to scale) of slice at z = 1

[GEOMETRY]
global "outer"

comp         : matid
    "media 0"  0
    "media 1"  1
    "media 2"  2

!#### LEVEL 1 UNIVERSE ####!

[UNIVERSE=general inner]
interior "pair"

[UNIVERSE][SHAPE=cuboid pair]
faces 0 2 0 100 0 100

[UNIVERSE][SHAPE=cuboid abs]
faces 0 1 0 100 0 100

[UNIVERSE][SHAPE=cuboid det]
faces 1 2 0 100 0 100

[UNIVERSE][CELL absorber]
comp "media 0"
shapes -abs

[UNIVERSE][CELL detector]
comp "media 1"
shapes -det


!#### LEVEL 0 UNIVERSE ####!

[UNIVERSE=general outer]
interior "full"

[UNIVERSE][SHAPE=cuboid full]
faces 0 100 0 100 0 100

[UNIVERSE][HOLE pair_0]
fill inner
translate 0 0 0

