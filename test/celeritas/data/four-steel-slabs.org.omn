[GEOMETRY]
global "global"
comp       : matid
    "G4_STAINLESS-STEEL" 0
    "G4_Galactic" 1

[UNIVERSE=general global]
interior "World"

!~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~!

[UNIVERSE][SHAPE=box]
name "box0"
widths 20 20 4

[UNIVERSE][SHAPE=box]
name "box1"
widths 20 20 4
translate 0 0 6

[UNIVERSE][SHAPE=box]
name "box2"
widths 20 20 4
translate 0 0 12

[UNIVERSE][SHAPE=box]
name "box3"
widths 20 20 4
translate 0 0 18

[UNIVERSE][SHAPE=box World]
widths 1000 1000 1000

!##############################################################################
! CELLS ("volumes")
!##############################################################################

[UNIVERSE][CELL]
name "box@0"
comp "G4_STAINLESS-STEEL"
shapes box0

[UNIVERSE][CELL]
name "box@1"
comp "G4_STAINLESS-STEEL"
shapes box1

[UNIVERSE][CELL]
name "box@2"
comp "G4_STAINLESS-STEEL"
shapes box2

[UNIVERSE][CELL]
name "box@3"
comp "G4_STAINLESS-STEEL"
shapes box3

[UNIVERSE][CELL World]
comp G4_Galactic
shapes World ~box0 ~box1 ~box2 ~box3
